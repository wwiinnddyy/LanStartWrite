{
  "name": "LanStartWrite",
  "version": "0.4.11",
  "description": "Simple Electron starter",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "test": "electron . --run-tests",
    "build": "electron-builder --win --x64 --publish never",
    "build:portable": "electron-builder --win portable --x64 --publish never",
    "build:installer": "electron-builder --win nsis --x64 --publish never",
    "build:all": "electron-builder --win --x64 --publish never"
  },
  "dependencies": {
    "semver": "^7.5.4",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8"
  }
}
